{
  "name": "gallerium",
  "version": "0.0.0-semantically-released",
  "description": "Responsive gallery logic",
  "main": "dist/index.js",
  "module": "dist/gallerium.esm.js",
  "jsnext:main": "dist/gallerium.esm.js",
  "browser": {
    "./dist/index.js": "./dist/index.js",
    "./dist/gallerium.esm.js": "./dist/gallerium.esm.js"
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s clean compile bundle",
    "bundle": "rollup -c",
    "check:format": "prettier --list-different \"**/*.{js,ts,tsx}\"",
    "check:types": "tsc --noEmit",
    "clean": "run-p clean:*",
    "clean:compiled": "del compiled",
    "clean:coverage": "del coverage",
    "clean:dist": "del dist",
    "compile": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"*.{js,ts,tsx}\"",
    "lint": "eslint . --ext .ts,.tsx",
    "postinstall": "npx patch-package",
    "postbundle": "del compiled && cp-cli ./index.js ./dist/index.js",
    "start": "run-p start:*",
    "start:storybook": "node scripts/startStorybook.js",
    "start:tdd": "node scripts/startTDD.js && yarn test:src --watch",
    "test": "run-s check:* lint build test:* && node scripts/jest/generateShield.js",
    "test:cjs": "jest --config ./scripts/jest/config.cjs.js",
    "test:cjsprod": "cross-env NODE_ENV=production jest --config ./scripts/jest/config.cjs.js",
    "test:es": "jest --config ./scripts/jest/config.es.js",
    "test:src": "jest --config ./scripts/jest/config.src.js",
    "test:umd": "jest --config ./scripts/jest/config.umd.js",
    "test:umdprod": "jest --config ./scripts/jest/config.umdprod.js",
    "typedoc": "typedoc --mode file --out website/static/api --theme minimal --readme none src/index.ts",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tbjgolden/gallerium.git"
  },
  "keywords": [
    "typescript"
  ],
  "author": "Tom Golden <github@tbjgolden.com>",
  "license": "MIT",
  "bugs": {
    "url": "github:tbjgolden/gallerium/issues"
  },
  "homepage": "github:tbjgolden/gallerium",
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/plugin-transform-runtime": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/preset-react": "7.12.10",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@storybook/addon-knobs": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "26.0.19",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.11.0",
    "babel-core": "6.26.3",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "classnames": "^2.2.6",
    "commitizen": "^4.2.2",
    "cp-cli": "2.0.0",
    "cross-env": "7.0.3",
    "dedent": "^0.7.0",
    "del-cli": "3.0.1",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "fast-glob": "^3.2.4",
    "fork-ts-checker-webpack-plugin": "^6.0.8",
    "fs-extra": "^9.0.1",
    "husky": "^4.3.6",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "patch-package": "^6.2.2",
    "prettier": "2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "17.0.1",
    "regenerator-runtime": "0.13.7",
    "rollup": "2.35.1",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-filesize": "9.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-size-snapshot": "0.12.0",
    "rollup-plugin-sourcemaps": "0.6.3",
    "rollup-plugin-terser": "7.0.2",
    "semantic-release": "^17.3.0",
    "ts-jest": "26.4.4",
    "ts-loader": "^8.0.12",
    "typedoc": "^0.19.2",
    "typedoc-plugin-markdown": "^3.1.1",
    "typescript": "4.1.3"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
